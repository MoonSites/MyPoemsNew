<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="LoginTextButton" parent="Widget.MaterialComponents.Button.TextButton.Icon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">55dp</item>
        <item name="cornerRadius">20dp</item>
        <!--<item title="strokeColor">@color/buttonShapeColor</item>-->
        <item name="android:textColor">?colorOnBackground</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_marginRight">5dp</item>
<!--        <item name="iconTint">@color/colorPrimaryDark</item>-->
    </style>

    <style name="EDasTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
<!--        <item title="android:textColor">@color/buttonTextColor</item>-->
        <item name="android:background">@android:color/transparent</item>
        <item name="android:textSize">16sp</item>
        <item name="android:imeOptions">flagNoExtractUi</item>
    </style>

    <style name="EditTextStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

</resources>